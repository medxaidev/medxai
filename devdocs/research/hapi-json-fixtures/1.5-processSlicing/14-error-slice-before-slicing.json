{
  "resourceType": "StructureDefinition",
  "id": "error-slice-before-slicing",
  "url": "http://example.org/fhir/StructureDefinition/ErrorSliceBeforeSlicing",
  "name": "ErrorSliceBeforeSlicing",
  "title": "ERROR CASE: Slice element appears before slicing declaration",
  "description": "The slice 'mrn' appears before the slicing declaration on Patient.identifier. HAPI should reject this.",
  "status": "active",
  "kind": "resource",
  "abstract": false,
  "type": "Patient",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Patient",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Patient.identifier:mrn",
        "path": "Patient.identifier",
        "sliceName": "mrn",
        "min": 1,
        "max": "1"
      },
      {
        "id": "Patient.identifier",
        "path": "Patient.identifier",
        "slicing": {
          "discriminator": [
            { "type": "value", "path": "system" }
          ],
          "rules": "open"
        }
      }
    ]
  }
}
