{
  "resourceType": "StructureDefinition",
  "id": "with-snapshot-and-differential",
  "url": "http://example.org/fhir/StructureDefinition/WithSnapshotAndDiff",
  "name": "WithSnapshotAndDiff",
  "title": "Complete SD with both snapshot and differential",
  "status": "active",
  "kind": "resource",
  "abstract": false,
  "type": "Patient",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Patient",
  "derivation": "constraint",
  "snapshot": {
    "element": [
      {
        "id": "Patient",
        "path": "Patient",
        "min": 0,
        "max": "*"
      },
      {
        "id": "Patient.id",
        "path": "Patient.id",
        "min": 0,
        "max": "1",
        "type": [{ "code": "http://hl7.org/fhirpath/System.String" }]
      },
      {
        "id": "Patient.name",
        "path": "Patient.name",
        "min": 1,
        "max": "*",
        "type": [{ "code": "HumanName" }],
        "mustSupport": true
      },
      {
        "id": "Patient.birthDate",
        "path": "Patient.birthDate",
        "min": 1,
        "max": "1",
        "type": [{ "code": "date" }],
        "mustSupport": true
      }
    ]
  },
  "differential": {
    "element": [
      {
        "id": "Patient.name",
        "path": "Patient.name",
        "min": 1,
        "mustSupport": true
      },
      {
        "id": "Patient.birthDate",
        "path": "Patient.birthDate",
        "min": 1,
        "mustSupport": true
      }
    ]
  }
}
