{
  "resourceType": "StructureDefinition",
  "id": "with-mapping",
  "url": "http://example.org/fhir/StructureDefinition/WithMapping",
  "name": "WithMapping",
  "title": "SD with mapping declarations to external systems",
  "status": "active",
  "kind": "resource",
  "abstract": false,
  "type": "Patient",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Patient",
  "derivation": "constraint",
  "mapping": [
    {
      "identity": "v2",
      "uri": "http://hl7.org/v2",
      "name": "HL7 v2 Mapping"
    },
    {
      "identity": "cda",
      "uri": "http://hl7.org/v3/cda",
      "name": "CDA (R2)"
    },
    {
      "identity": "hospital-adt",
      "uri": "http://hospital.example.org/adt",
      "name": "Hospital ADT System"
    }
  ],
  "differential": {
    "element": [
      {
        "id": "Patient.identifier",
        "path": "Patient.identifier",
        "min": 1,
        "mapping": [
          { "identity": "v2", "map": "PID-3" },
          { "identity": "hospital-adt", "map": "patient.mrn" }
        ]
      },
      {
        "id": "Patient.name",
        "path": "Patient.name",
        "min": 1,
        "mapping": [
          { "identity": "v2", "map": "PID-5, PID-9" },
          { "identity": "cda", "map": ".patient.name" }
        ]
      }
    ]
  }
}
