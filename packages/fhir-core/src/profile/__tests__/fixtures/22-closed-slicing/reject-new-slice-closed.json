    { "path": "Patient", "min": 0, "max": "*" },
    { "path": "Patient.identifier", "min": 0, "max": "*", "slicing": { "discriminator": [{ "type": "value", "path": "system" }], "rules": "closed" } },
    { "path": "Patient.identifier", "sliceName": "MRN", "min": 0, "max": "1" },
    { "path": "Patient.name", "min": 0, "max": "*" }
