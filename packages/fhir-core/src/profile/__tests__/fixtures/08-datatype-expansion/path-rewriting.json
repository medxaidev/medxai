{
  "description": "Datatype expansion: verify path rewriting from datatype namespace to target",
  "base": [
    { "path": "Patient", "min": 0, "max": "*" },
    { "path": "Patient.identifier", "min": 0, "max": "*", "type": [{ "code": "Identifier" }] }
  ],
  "diff": [
    { "path": "Patient.identifier.value", "min": 1, "short": "MRN value" }
  ],
  "datatypeCache": {
    "http://hl7.org/fhir/StructureDefinition/Identifier": [
      { "path": "Identifier", "min": 0, "max": "*" },
      { "path": "Identifier.system", "min": 0, "max": "1" },
      { "path": "Identifier.value", "min": 0, "max": "1" }
    ]
  },
  "expected": {
    "hasValueElement": true,
    "valuePath": "Patient.identifier.value",
    "valueMin": 1,
    "valueShort": "MRN value"
  }
}
