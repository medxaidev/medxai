{
  "description": "Case A: Diff introduces slicing on an unsliced base element",
  "baseElements": [
    { "path": "Patient", "min": 0, "max": "*" },
    { "path": "Patient.identifier", "min": 0, "max": "*" },
    { "path": "Patient.identifier.system", "min": 0, "max": "1" },
    { "path": "Patient.identifier.value", "min": 0, "max": "1" },
    { "path": "Patient.name", "min": 0, "max": "*" }
  ],
  "diffElements": [
    { "path": "Patient.identifier", "slicing": { "discriminator": [{ "type": "value", "path": "system" }], "rules": "open", "ordered": false } },
    { "path": "Patient.identifier", "sliceName": "MRN", "min": 1, "max": "1" }
  ],
  "expected": {
    "hasSlicingRoot": true,
    "slicingRules": "open",
    "sliceCount": 1,
    "sliceNames": ["MRN"]
  }
}
