{
  "description": "Closed slicing with unmatched values → SLICING_NO_MATCH error",
  "slicingRoot": {
    "path": "Patient.identifier",
    "id": "Patient.identifier",
    "min": 0, "max": "unbounded",
    "types": [{ "code": "Identifier" }],
    "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false,
    "slicing": {
      "discriminators": [{ "type": "value", "path": "system" }],
      "rules": "closed",
      "ordered": false
    }
  },
  "slices": [
    {
      "path": "Patient.identifier",
      "id": "Patient.identifier:MRN",
      "min": 0, "max": 1,
      "types": [{ "code": "Identifier" }],
      "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false,
      "sliceName": "MRN",
      "fixed": { "system": "http://hospital.example.org/mrn" }
    }
  ],
  "scenarios": [
    {
      "label": "matched value only → valid",
      "values": [
        { "system": "http://hospital.example.org/mrn", "value": "12345" }
      ],
      "expectedIssueCount": 0
    },
    {
      "label": "unmatched value in closed slicing → SLICING_NO_MATCH",
      "values": [
        { "system": "http://hospital.example.org/mrn", "value": "12345" },
        { "system": "http://unknown.org", "value": "99999" }
      ],
      "expectedIssueCount": 1,
      "expectedIssueCodes": ["SLICING_NO_MATCH"]
    },
    {
      "label": "only unmatched values → SLICING_NO_MATCH",
      "values": [
        { "system": "http://unknown.org", "value": "99999" }
      ],
      "expectedIssueCount": 1,
      "expectedIssueCodes": ["SLICING_NO_MATCH"]
    }
  ]
}
