{
  "description": "Type mismatch scenarios: wrong JS type for the expected FHIR type",
  "scenarios": [
    {
      "label": "number value for string type → TYPE_MISMATCH",
      "element": {
        "path": "Patient.gender",
        "id": "Patient.gender",
        "min": 0, "max": 1,
        "types": [{ "code": "string" }],
        "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false
      },
      "value": 42,
      "expectedIssueCount": 1,
      "expectedIssues": [{ "code": "TYPE_MISMATCH" }]
    },
    {
      "label": "string value for boolean type → TYPE_MISMATCH",
      "element": {
        "path": "Patient.active",
        "id": "Patient.active",
        "min": 0, "max": 1,
        "types": [{ "code": "boolean" }],
        "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false
      },
      "value": "true",
      "expectedIssueCount": 1,
      "expectedIssues": [{ "code": "TYPE_MISMATCH" }]
    },
    {
      "label": "decimal value for integer type → TYPE_MISMATCH",
      "element": {
        "path": "ElementDefinition.min",
        "id": "ElementDefinition.min",
        "min": 0, "max": 1,
        "types": [{ "code": "integer" }],
        "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false
      },
      "value": 3.14,
      "expectedIssueCount": 1,
      "expectedIssues": [{ "code": "TYPE_MISMATCH" }]
    },
    {
      "label": "boolean value for integer type → TYPE_MISMATCH",
      "element": {
        "path": "ElementDefinition.min",
        "id": "ElementDefinition.min",
        "min": 0, "max": 1,
        "types": [{ "code": "integer" }],
        "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false
      },
      "value": false,
      "expectedIssueCount": 1,
      "expectedIssues": [{ "code": "TYPE_MISMATCH" }]
    },
    {
      "label": "string value for Coding type → TYPE_MISMATCH",
      "element": {
        "path": "Observation.code",
        "id": "Observation.code",
        "min": 1, "max": 1,
        "types": [{ "code": "CodeableConcept" }],
        "constraints": [], "mustSupport": false, "isModifier": false, "isSummary": false
      },
      "value": "some-code",
      "expectedIssueCount": 1,
      "expectedIssues": [{ "code": "TYPE_MISMATCH" }]
    }
  ]
}
