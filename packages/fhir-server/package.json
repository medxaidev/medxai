{
  "name": "@medxai/fhir-server",
  "version": "0.0.1",
  "description": "MedXAI FHIR Server",
  "homepage": "https://medxai.com.cn",
  "license": "Apache-2.0",
  "author": "Fangjun <fangjun20208@gmail.com>",
  "keywords": [
    "typescript",
    "FHIR",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medxaidev/medxai.git",
    "directory": "packages/fhir-server"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && node ./scripts/esbuild.mjs",
    "dev": "tsx watch ./src/index.ts",
    "start": "node ./dist/esm/index.mjs",
    "test": "vitest"
  },
  "dependencies": {
    "@medxai/fhir-core": "0.0.1",
    "@medxai/fhir-persistence": "0.0.1",
    "bcryptjs": "^3.0.3",
    "fastify": "5.7.4",
    "jose": "^6.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6"
  }
}
