{
  "name": "@medxai/fhir-persistence",
  "version": "0.0.1",
  "description": "MedXAI FHIR Persistence",
  "homepage": "https://medxai.com.cn",
  "license": "Apache-2.0",
  "author": "Fangjun <fangjun20208@gmail.com>",
  "keywords": [
    "typescript",
    "FHIR",
    "core"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medxaidev/medxai.git",
    "directory": "packages/fhir-persistence"
  },
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && node ./scripts/esbuild.mjs",
    "test": "vitest",
    "schema:generate": "tsx src/cli/generate-schema.ts --spec-dir ../../spec/fhir/r4 --output schema.sql",
    "db:init": "tsx scripts/init-db.ts",
    "db:reset": "tsx scripts/init-db.ts --reset",
    "db:sql": "tsx scripts/init-db.ts --sql-only"
  },
  "dependencies": {
    "@medxai/fhir-core": "0.0.1",
    "pg": "8.18.0"
  },
  "devDependencies": {
    "@types/pg": "8.16.0"
  },
  "peerDependencies": {}
}
